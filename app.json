{
  "expo": {
    "name": "ReaPad",
    "slug": "ReaPad",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nicomellon.reapad"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.nicomellon.reapad"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
